void setup(void)
{
  Wire.begin();

  Serial.begin(9600);

  //attaching Servo to appropriate location and setting it to default
  myServo.attach(3);
  myServo.write(90);

  //section from analog write series from fabacademy H-Bridge Documentation
  analogWriteFreq(100000);
  analogWriteRange(65535);

  //testing if screen is there
  Serial.println("VL53L1X Qwiic Test");
     if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  //testing if distance sensor is there
  if (distanceSensor.begin() != 0) //Begin returns 0 on a good init
  {
    Serial.println("Sensor failed to begin. Please check wiring. Freezing...");
    while (1)
      ;
  }
  Serial.println("Sensor online!");

  //setting default eye display and clearing ADAFruit logo.
  display.clearDisplay();
  display.drawBitmap(0, 0, EYES_OPEN_array, 128, 64, 1);
  display.display();
  delay(500); // Pause for half second
}

void loop(void)
{
  delay(1000);
  //setting motors to zero power if nothing happens
  analogWrite(4,0);
  analogWrite(2,0);

  distanceSensor.startRanging(); //Write configuration bytes to initiate measurement
  while (!distanceSensor.checkForDataReady())
  {
    delay(1);
  }
  int distance = distanceSensor.getDistance(); //Get the result of the measurement from the sensor
  distanceSensor.clearInterrupt();
  distanceSensor.stopRanging();
  
  //Animation for intro to video
  //DAPR Winks:
  display.clearDisplay();
  display.drawBitmap(0, 0, EYES_OPEN_array, 128, 64, 1);
